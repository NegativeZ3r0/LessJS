{
  "devDependencies": {
    "@tailwindcss/cli": "^4.1.18",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "tailwindcss": "^4.1.18"
  },
  "scripts": {
    "build:css": "npx @tailwindcss/cli -i ./static/input.css -o ./static/output.css --watch",
    "server:dev": "npx nodemon -e c,h,mustache --exec \"make && tmp/fioapp -w 1\"",
    "dev": "concurrently \"npm run build:css\" \"npm run server:dev\""
  }
}
